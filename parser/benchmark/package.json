{
  "name": "mixpad-benchmark",
  "version": "1.0.0",
  "description": "Performance benchmarking for Mixpad scanner vs competitors",
  "main": "runner.js",
  "scripts": {
    "build": "npx esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/benchmark.bundle.js --target=node16",
    "bench": "npm run build && node --expose-gc dist/benchmark.bundle.js",
    "bench:readme": "npm run build && node --expose-gc dist/benchmark.bundle.js --update-readme"
  },
  "keywords": ["benchmark", "markdown", "parser", "performance"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.18.0",
    "@types/node": "^18.0.0"
  }
}
